  "bugs": {
    "url": "https://github.com/umbe77/yasb/issues"
  },
  "homepage": "https://github.com/umbe77/yasb#readme",
  "devDependencies": {
    "@types/node": "^15.12.1",
    "concurrently": "^6.2.0",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "dotenv": "^10.0.0"
  }
